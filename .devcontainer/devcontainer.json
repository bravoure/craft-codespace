{
	"name": "Craft Codespace",
	"dockerComposeFile": "../docker-compose.yml",
	"service": "workspace",
	"runServices": [
		"mariadb",
	],
	"remoteUser": "codespace",
	"extensions": [
		"GitHub.vscode-pull-request-github",
		"ms-azuretools.vscode-docker",
		"whatwedo.twig"
	],
	"workspaceFolder": "/workspace",
	"portsAttributes": {
		"8080": {
			"label": "Craft"
		}
	},
	"onCreateCommand": "docker compose pull"
}
